{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:read-all",
    {
      "identifier": "fs:scope",
      "allow": [
        "**"
      ]
    },
    "store:default",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "exec-cmd",
          "cmd": "cmd.exe",
          "args": true
        },
        {
          "name": "exec-sh",
          "cmd": "sh",
          "args": true
        }
      ]
    },
    "os:default"
  ]
}